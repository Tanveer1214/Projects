<?xml version="1.0" encoding="UTF-8"?>
<beans
xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns:context="http://www.springframework.org/schema/context"
xmlns:util="http://www.springframework.org/schema/util"
xmlns:p="http://www.springframework.org/schema/p"
xsi:schemaLocation="http://www.springframework.org/schema/beans 
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
http://www.springframework.org/schema/context 
http://www.springframework.org/schema/context/spring-context.xsd
http://www.springframework.org/schema/util 
http://www.springframework.org/schema/util/spring-util.xsd">

	<util:list list-class="java.util.ArrayList" id="city">
		<value>Karachi</value>
		<value>Lahore</value>
		<value>Islamabad</value>
		<value>Queta</value>
		<value>Mehrabpur</value>
	</util:list>
   
	<context:component-scan base-package="com.springcore.stereo"/>
	
	<bean class="com.springcore.stereo.Pro" name="pro" scope="prototype"/>
   
</beans>
