<?xml version="1.0" encoding="UTF-8"?>
<beans
xmlns="http://www.springframework.org/schema/beans"
xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns:p="http://www.springframework.org/schema/p"
xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

<!-- following 3 ways to use setter inject -->
  <bean class="com.springcore.Student" name="temp1">
 		<property name="id">
 		   <value>10</value>
 		</property>
 		
 		<property name="name">
 		   <value>Tanveer Hussain</value>
 		</property>
 		
 		<property name="city">
 		   <value>Mehrabpur</value>
 		</property>
</bean> 
  
  <bean class="com.springcore.Student" name="temp2">
  	<property name="id" value="20"/>
  	<property name="name" value="Sammer Hussain"/>
  	<property name="city" value="Mehrabpur"/>
  </bean>
  
  <bean class="com.springcore.Student" name="temp3" p:id="30" p:name="Sagar" p:city="Nawabshah"/>
   
</beans>
